        "info_block_iphyCM": {
          "type": "info_block",
          "settings": {
            "text": "Frosted glass bowl",
            "image": "shopify://shop_images/Screenshot_2021-03-09_at_9.11.10_PM.png",
            "textarea": "With an elegant frosted glass finish, your dish also doubles as a dinner party-worthy centerpiece. Serve it straight from microwave to table — see if your guests can guess how you cooked it!",
