    }

    .select.open .select__head {
      border-bottom: unset;  
    }

    .select__head::after {
      width: 21px;
      height: 13px;
      background: url(data:image/png;base64,) no-repeat transparent;
      position: absolute;
      right: 12px;
      content: "";
      display: block;
      background-size: contain;
      transition: .2s;
