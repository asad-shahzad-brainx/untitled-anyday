  #shopify-section-{{ section.id }} .recipe__info-settings {
    display: block;
    padding: 22px;
    padding-top: 10px;
    background-color: #bed8fc;

    .select {
      width: auto;
      min-width: 130px;
      padding: 0;
      position: relative;
    }

    .select__head {
      width: 100%;
      height: 43px;
      cursor: pointer;
      color: #000;
      font-size: 18px;
      font-weight: 400;
      border: 1px solid #121d29;
      background-color: #fff;
